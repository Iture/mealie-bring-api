        "recipe_instructions": [
            {
                "id": "1bd04d4f-96e0-401e-9c6c-dfce16c66dee",
                "title": "",
                "text": "Die Zwiebeln w\u00fcrfeln und in \u00d6l anschwitzen.",
                "ingredient_references": [
                    {
                        "reference_id": "1d7ff9c6-aa5b-4005-b040-3a6b5ab5ca99"
                    }
                ]
            },
            {
                "id": "824bdb6a-26e3-4c02-be07-906c17497b79",
                "title": "",
                "text": "Den Knoblauch und Reis dazugeben und bei st\u00e4ndigem R\u00fchren anschwitzen.",
                "ingredient_references": [
                    {
                        "reference_id": "d631877c-cec2-4c17-ac86-36137ac000cc"
                    },
                    {
                        "reference_id": "a92e94be-ec50-421d-bf21-cc1324163032"
                    }
                ]
            },
            {
                "id": "38d62cc3-5659-4c81-a259-9b2045308c15",
                "title": "",
                "text": "Gem\u00fcsebr\u00fche aufsetzen.",
                "ingredient_references": [
                    {
                        "reference_id": "bf0aab1c-b23c-4608-9f0d-91301a56d1f7"
                    }
                ]
            },
            {
                "id": "c6198c29-b7d1-4dee-96e3-b24cc804f666",
                "title": "",
                "text": "Mit dem Wein abl\u00f6schen.",
                "ingredient_references": [
                    {
                        "reference_id": "7d80cb5c-6b98-4787-b0ba-e2b9c2b1cf57"
                    }
                ]
            },
            {
                "id": "f1dc3dff-e67e-4eef-bcce-a18db667226a",
                "title": "",
                "text": "Wenn der Wein verkocht ist, die Br\u00fche kellenweise zugeben und umr\u00fchren.",
                "ingredient_references": [
                    {
                        "reference_id": "bf0aab1c-b23c-4608-9f0d-91301a56d1f7"
                    }
                ]
            },
            {
                "id": "57c531b7-ff5c-4d8b-90ee-b995ffd69fa1",
                "title": "",
                "text": "W\u00e4hrenddessen den Lachs abtupfen, etwas Salz in ihn einarbeiten und langsam anbraten.",
                "ingredient_references": [
                    {
                        "reference_id": "d864e6ef-c404-4543-b394-587ff0603d9c"
                    }
                ]
            },
            {
                "id": "21ba1cdc-fdde-42ee-8500-62efd52bffd6",
                "title": "",
                "text": "Sobald der Reis gar ist (nach ca. 25 Minuten,  er sollte noch Biss haben), Zitronenschale und -saft, Butter, Parmesan und Petersilie unterr\u00fchren und mit Salz und Pfeffer abschmecken.",
                "ingredient_references": [
                    {
                        "reference_id": "cb3ac321-d47f-46dd-97f2-96e0facd847e"
                    },
                    {
                        "reference_id": "813ed8c4-145e-47f3-bdc8-17f434baace1"
                    },
                    {
                        "reference_id": "64ae90b2-70e3-480a-bac1-3a5648e16bc2"
                    },
                    {
                        "reference_id": "040ee614-ebf6-4033-af82-2de2315debf8"
                    }
                ]
            }
